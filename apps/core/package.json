{
  "name": "core",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nest build core",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf *.tsbuildinfo",
    "dev": "yarn cognito-local & nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tsc --noEmit && TIMING=1 eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "repl": "nest start --entryFile repl --watch",
    "test": "yarn cognito-local & jest --coverage",
    "test:watch": "yarn cognito-local & jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.259.0",
    "@aws-sdk/lib-dynamodb": "^3.259.0",
    "@fastify/static": "^6.6.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/swagger": "^6.2.1",
    "aws-jwt-verify": "^3.4.0",
    "core-types": "*",
    "nanoid": "3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.272.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@shelf/jest-dynamodb": "^3.4.1",
    "cognito-local": "^3.21.1",
    "dynamodb-local": "^0.0.31",
    "eslint-config-custom": "*",
    "jest-config": "*",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "tsconfig": "*",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  }
}
