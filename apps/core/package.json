{
  "name": "core",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nest build core",
    "clean": "rimraf .turbo && rimraf dist && rimraf *.tsbuildinfo",
    "clean:full": "rimraf .turbo && rimraf node_modules && rimraf dist && rimraf *.tsbuildinfo",
    "dev": "yarn cognito-local & nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tsc --noEmit && TIMING=1 eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "lint:commit": "tsc --noEmit && TIMING=1 eslint $(git diff --name-only HEAD HEAD~1 | grep -E \"src/**/*.ts\" | xargs) --max-warnings 0",
    "lint:fix": "eslint \"{src,test}/**/*.ts*\" --fix",
    "repl": "nest start --entryFile repl --watch",
    "test": "jest --coverage",
    "test:commit": "jest --lastCommit --coverage=false",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.470.0",
    "@aws-sdk/client-iotsitewise": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "3.470.0",
    "@aws-sdk/client-iotsitewise": "3.470.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^6.12.0",
    "@fastify/view": "^8.2.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.4.3",
    "@nestjs/platform-fastify": "^9.4.3",
    "@nestjs/swagger": "^7.1.15",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/throttler": "^4.2.1",
    "aws-jwt-verify": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dynamodb-local": "^0.0.32",
    "handlebars": "^4.7.8",
    "nanoid": "3.1.31",
    "nestjs-pino": "^3.5.0",
    "pino-http": "^8.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@shelf/jest-dynamodb": "^3.4.2",
    "aws-sdk-client-mock": "^3.0.0",
    "cognito-local": "^3.23.2",
    "eslint-config-custom": "*",
    "eslint-plugin-jest": "^27.6.0",
    "jest-config": "*",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.5.1",
    "tsconfig": "*",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  }
}
