{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.js",
    "build": "DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf dist && rimraf build && rimraf *.tsbuildinfo",
    "dev": "DISABLE_ESLINT_PLUGIN=true BROWSER=none PORT=3001 react-app-rewired start",
    "gen:intl": "formatjs extract \"src/**/*.ts*\" --ignore=\"**/*.d.ts\" --out-file lang/en.json --id-interpolation-pattern \"[sha512:contenthash:base64:6]\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\" --max-warnings 0",
    "lint:commit": "tsc --noEmit && TIMING=1 eslint $(git diff --name-only HEAD HEAD~1 | grep -E \"src/**/*.ts*\" | xargs) --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "test": "vitest run --coverage",
    "test:commit": "vitest run --changed HEAD~1",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^4.6.1",
    "@aws-sdk/client-iot-events": "^3.332.0",
    "@aws-sdk/client-iotsitewise": "^3.332.0",
    "@aws-sdk/credential-providers": "^3.332.0",
    "@cloudscape-design/collection-hooks": "^1.0.21",
    "@cloudscape-design/components": "^3.0.280",
    "@cloudscape-design/global-styles": "^1.0.9",
    "@iot-app-kit/dashboard": "^5.7.0",
    "@tanstack/react-query": "^4.29.5",
    "@vitest/coverage-c8": "^0.30.1",
    "aws-amplify": "^5.2.0",
    "jotai": "^2.1.0",
    "nanoid": "3.1.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-hook-form": "^7.43.7",
    "react-intl": "^6.4.2",
    "react-router-dom": "^6.11.1",
    "react-use": "^17.4.0",
    "tiny-invariant": "^1.3.1",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@aws-sdk/client-sts": "^3.328.0",
    "@formatjs/cli": "^6.1.1",
    "@formatjs/ts-transformer": "^3.13.1",
    "@hookform/devtools": "^4.3.1",
    "@tanstack/eslint-plugin-query": "^4.29.0",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.31.0",
    "babel-plugin-formatjs": "^10.5.1",
    "customize-cra": "^1.0.0",
    "eslint-config-custom": "*",
    "eslint-plugin-formatjs": "^4.10.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "identity-obj-proxy": "^3.0.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.3",
    "tsconfig": "*",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
