{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "DISABLE_ESLINT_PLUGIN=true vite build",
    "serve": "vite preview",
    "analyze": "source-map-explorer build/static/js/*.js",
    "clean": "rimraf .turbo && rimraf dist && rimraf build && rimraf --glob *.tsbuildinfo",
    "clean:full": "rimraf .turbo && rimraf node_modules && rimraf dist && rimraf build && rimraf --glob *.tsbuildinfo",
    "predev": "mkdir -p ./build && cp ./service-worker.js ./build/service-worker.js",
    "dev": "yarn build && DISABLE_ESLINT_PLUGIN=true BROWSER=none PORT=3001 vite",
    "gen:intl": "formatjs extract \"src/**/*.ts*\" --ignore=\"**/*.d.ts\" --out-file lang/en.json --id-interpolation-pattern \"[sha512:contenthash:base64:6]\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\" --max-warnings 0",
    "lint:commit": "tsc --noEmit && TIMING=1 eslint $(git diff --name-only HEAD HEAD~1 | grep -E \"src/**/*.(ts|tsx|css)\" | xargs) --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "test": "vitest run --coverage",
    "test:commit": "vitest run --changed HEAD~1",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^5.3.2",
    "@aws-sdk/client-cloudwatch": "3.515.0",
    "@aws-sdk/client-cloudwatch-logs": "3.515.0",
    "@aws-sdk/client-iot-events": "3.515.0",
    "@aws-sdk/client-iotsitewise": "3.515.0",
    "@aws-sdk/credential-providers": "3.515.0",
    "@cloudscape-design/collection-hooks": "^1.0.37",
    "@cloudscape-design/component-toolkit": "^1.0.0-beta.38",
    "@cloudscape-design/components": "^3.0.518",
    "@cloudscape-design/design-tokens": "3.0.34",
    "@cloudscape-design/global-styles": "^1.0.23",
    "@iot-app-kit/core": "10.8.0",
    "@iot-app-kit/dashboard": "10.8.0",
    "@tanstack/react-query": "^5.24.1",
    "aws-amplify": "^5.3.11",
    "axios": "^1.6.8",
    "cytoscape": "^3.28.1",
    "jotai": "^2.6.4",
    "nanoid": "3.1.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.49.3",
    "react-intl": "^6.5.5",
    "react-router-dom": "^6.21.3",
    "react-use": "^17.4.2",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.1",
    "vite": "^5.1.5",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@aws-sdk/client-sts": "^3.450.0",
    "@formatjs/cli": "^6.2.7",
    "@formatjs/ts-transformer": "^3.13.9",
    "@hookform/devtools": "^4.3.1",
    "@originjs/vite-plugin-federation": "^1.3.3",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@tanstack/react-query-devtools": "^5.24.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^1.1.1",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "babel-plugin-formatjs": "^10.5.13",
    "customize-cra": "^1.0.0",
    "eslint-config-custom": "*",
    "eslint-plugin-formatjs": "^4.11.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^23.0.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "sass": "^1.70.0",
    "source-map-explorer": "^2.5.3",
    "tsconfig": "*",
    "typescript": "^5.2.2",
    "vite-plugin-commonjs": "^0.10.1",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vitest": "^0.34.4"
  },
  "resolutions": {
    "@iot-app-kit/dashboard/**/@cloudscape-design/component-toolkit": "1.0.0-beta.27"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
