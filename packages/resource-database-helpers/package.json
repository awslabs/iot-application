{
  "name": "resource-database-helpers",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.259.0",
    "@aws-sdk/lib-dynamodb": "^3.259.0"
  },
  "devDependencies": {
    "@aws-sdk/util-dynamodb": "^3.264.0",
    "@types/jest": "29.2.4",
    "jest": "29.3.1",
    "ts-jest": "29.0.3",
    "aws-sdk-client-mock": "^2.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
